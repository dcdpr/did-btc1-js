{
    "tsconfig": "./tsconfig.json",
    "entryPoints": [
        "./src/index.ts"
    ],
    "plugin": [
        "typedoc-plugin-markdown"
    ],
    "theme": "markdown",
    "out": "../../docs/packages/cryptosuite",
    "exclude": [
        "**/lib/**",
        "**/dist/**",
        "**/tests/**",
        "**/node_modules/**"
    ],
    "excludePrivate": true,
    "excludeProtected": true,
    "excludeExternals": true,
    "hideGenerator": true,
}